<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="BluetoothSerial">
    <param name="android-package" value="com.megster.cordova.BluetoothSerial"/>
  </feature>

  <feature name="Device">
    <param name="android-package" value="org.apache.cordova.device.Device"/>
  </feature>

  <feature name="Printer">
    <param name="android-package" value="de.appplant.cordova.plugin.printer.Printer"/>
  </feature>

  <feature name="StarPRNT">
    <param name="android-package" value="starprnt.cordova.StarPRNT"/>
  </feature>

  <feature name="ThermalPrinter">
    <param name="android-package" value="de.paystory.thermal_printer.ThermalPrinterCordovaPlugin"/>
  </feature>

  
</widget>